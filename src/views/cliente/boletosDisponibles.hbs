<br><br>
<h4>Vuelos TEC Plane</h4>
<br><br>
<div class="row">
    <br><br>
    {{#each vuelosEncontrados}}
    <div class="col-md-3">
        <div class="vuelos">
            <div class="cuerpo">
                <h5 class="nombre">
                  IdVuelo:  {{idVuelo}}
                </h5>
                <p>Nombre: {{nombre}}</p>
                <p>Origen {{origen}}</p>
                <p>Destino: {{destino}}</p>
                <p>Fecha de ida: {{fechaIda}}</p>
                <p>Fecha de vuelta: {{fechaVuelta}}</p>
                <p>Precio {{precio}}</p>
                <p>Restricciones: {{restricciones}}</p>
                <p>Servicios: {{servicios}}</p>
                <p>Estado: {{estado}}</p>
                <p>Maximos: {{maximo}}</p>
                <p>Disponibles: {{disponibles}}</p>
                <p>Boletos: {{boletos}}</p>
                <div>
                <form action="/cliente/boletos/seleccionar" method="GET">
	              <button class="btn btn-secondary" type="submit"> Comprar</button>
                </form>
                <br>
                <br> 
                </div>
                <br><br>
            </div>
        </div>
    </div>
    {{/each}}
</div>
<br><br><br>
