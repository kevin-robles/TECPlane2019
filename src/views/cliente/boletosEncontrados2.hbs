<br><br>
<h4>Clientes TEC Plane</h4>
<br><br>
<div class="row">
    <br><br>
    {{#each vuelosFinales}}
    <form action="/cliente/boletos/seleccionar" method="GET"> 
    <div class="col-md-3">
        <div class="vuelos">
            <div class="cuerpo">
                <h5 class="nombre">
                  IdVuelo:  {{idVuelo}}
                </h5>
                <p>Nombre: {{nombre}}</p>
                <p>Origen {{origen}}</p>
                <p>Destino: {{destino}}</p>
                <p>Fecha de ida: {{fechaIda}}</p>
                <p>Fecha de vuelta: {{fechaVuelta}}</p>
                <p>Precio {{precio}}</p>
                <p>Restricciones: {{restricciones}}</p>
                <p>Servicios: {{servicios}}</p>
                <p>Estado: {{estado}}</p>
                <p>Maximos: {{maximo}}</p>
                <p>Disponibles: {{disponibles}}</p>
                <br><br>         
                <button class="btn btn-primary" type="submit"> Comprar</button>
            </div>
        </div>
    </div>
</form>
    {{/each}}
</div>
<br><br><br>