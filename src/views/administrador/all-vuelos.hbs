
<br><br>
<h4>Vuelos TEC Plane</h4>
<br><br>
<div class="row">
    <br><br>
    {{#each vuelos}}
    <div class="col-md-3">
        <div class="vuelos">
            <div class="cuerpo">
                <h5 class="nombre">
                  IdVuelo:  {{idVuelo}}
                </h5>
                <p>Nombre: {{nombre}}</p>
                <p>Nombre Aerolínea: {{nombreAerolinea}}</p>
                <p>Origen {{origen}}</p>
                <p>Destino: {{destino}}</p>
                <p>Fecha de ida: {{fechaIda}}</p>
                <p>Fecha de vuelta: {{fechaVuelta}}</p>
                <p>Precio {{precio}}</p>
                <p>Restricciones: {{restricciones}}</p>
                <p>Servicios: {{servicios}}</p>
                <p>Estado: {{estado}}</p>
                <p>Maximos: {{maximo}}</p>
                <p>Disponibles: {{disponibles}}</p>
                
                <br><br>
            </div>
        </div>
    </div>
    {{/each}}
    {{#if vueloSolo}}
    <div class="col-md-3">
        <div class="vuelos">
            <div class="cuerpo">
                <h5 class="nombre">
                  IdVuelo:  {{vueloSolo.idVuelo}}
                </h5>
                <p>Nombre: {{vueloSolo.nombre}}</p>
                <p>Nombre Aerolínea: {{vueloSolo.nombreAerolinea}}</p>
                <p>Origen {{vueloSolo.origen}}</p>
                <p>Destino: {{vueloSolo.destino}}</p>
                <p>Fecha de ida: {{vueloSolo.fechaIda}}</p>
                <p>Fecha de vuelta: {{vueloSolo.fechaVuelta}}</p>
                <p>Precio {{vueloSolo.precio}}</p>
                <p>Restricciones: {{vueloSolo.restricciones}}</p>
                <p>Servicios: {{vueloSolo.servicios}}</p>
                <p>Estado: {{vueloSolo.estado}}</p>
                <p>Maximos: {{vueloSolo.maximo}}</p>
                <p>Disponibles: {{vueloSolo.disponibles}}</p>
                
                <br><br>
            </div>
        </div>
    </div>
    {{/if}}
</div>
<br><br><br>