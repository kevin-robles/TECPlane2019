<br><br>
<h4>Aeropuertos TEC Plane</h4>
<br><br>
<div class="row">
    <br><br>
    {{#each aeropuertos}}
    <div class="col-md-3">
        <div class="aeropuerto">
            <div class="cuerpo">
                <h5 class="nombre">
                  Nombre:  {{nombre}}
                </h5>
                <p>Localización: {{localizacion}}</p>
                <p>Contacto: </p>{{contacto}}</p>
                <p>sitioWeb: {{sitioWeb}}</p>
                <br><br>
            </div>
        </div>
    </div>
    {{/each}}

    {{#each resultadoFinal}}
    <div class="col-md-7">
        <div class="aeropuerto">
            <div class="cuerpo">
                <h5 class="nombre">
                  Nombre:  {{nombre}}
                </h5>
                <p>Localización: {{localizacion}}</p>
                <p>Contacto: </p>{{contacto}}</p>
                <p>sitioWeb: {{sitioWeb}}</p>
                <br><br>
            </div>
        </div>
    </div>
    {{/each}}
</div>
<br><br><br>